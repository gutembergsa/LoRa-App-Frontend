{
  "name": "lora-project-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "css-build": "node-sass --omit-source-map-url ./sass/mystyles.scss ./css/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "postinstall": "npm run build",
    "start": "node server.js && npm run css-watch"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "buefy": "^0.8.6",
    "bulma": "^0.8.0",
    "mqtt": "^3.0.0",
    "register-service-worker": "^1.6.2",
    "restify": "^8.4.0",
    "vue": "^2.6.10",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-pwa": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "chart.js": "^2.8.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
